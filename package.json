{
  "name": "webpack-react-boilerplate",
  "version": "1.0.0",
  "description": "",
  "main": "./src/index.js",
  "scripts": {
    "build": "webpack --env.NODE_ENV=prod --env.mode=production --config ./webpack/webpack.config.js",
    "clean": "rm -rf ./build",
    "dev": "webpack-dev-server --env.NODE_ENV=local --env.mode=development --config ./webpack/webpack.config.js",
    "prod": "npm run clean; npm run build",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "lint": "eslint src",
    "simulate": "node ./servers/dev.js"
  },
  "keywords": [
    "react",
    "boilerplate",
    "webpack"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "7.5.5",
    "@babel/core": "7.5.5",
    "@babel/plugin-proposal-class-properties": "7.5.5",
    "@babel/preset-env": "7.5.5",
    "@babel/preset-react": "7.0.0",
    "autoprefixer": "9.6.1",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "10.0.2",
    "babel-jest": "24.8.0",
    "babel-loader": "8.0.6",
    "css-loader": "3.1.0",
    "css-mqpacker": "7.0.0",
    "cssnano": "4.1.10",
    "enzyme": "3.10.0",
    "enzyme-adapter-react-16": "1.14.0",
    "enzyme-to-json": "3.3.5",
    "eslint": "6.0.1",
    "eslint-plugin-react": "7.14.3",
    "file-loader": "4.1.0",
    "html-webpack-plugin": "4.0.0-alpha",
    "image-webpack-loader": "5.0.0",
    "jest": "24.8.0",
    "mini-css-extract-plugin": "0.8.0",
    "node-sass": "4.12.0",
    "postcss-loader": "3.0.0",
    "react-hot-loader": "4.11.1",
    "redux-logger": "3.0.6",
    "redux-mock-store": "1.5.3",
    "sass-loader": "7.1.0",
    "style-loader": "0.23.1",
    "uglifyjs-webpack-plugin": "2.1.3",
    "url-loader": "2.1.0",
    "webpack": "4.37.0",
    "webpack-cli": "3.3.6",
    "webpack-dev-server": "3.7.2",
    "webpack-merge": "4.2.1"
  },
  "dependencies": {
    "@babel/polyfill": "7.4.4",
    "@feathersjs/express": "1.2.7",
    "@feathersjs/feathers": "3.2.3",
    "prop-types": "15.7.2",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "react-redux": "7.1.0",
    "react-router-dom": "5.0.1",
    "redux": "4.0.4",
    "redux-thunk": "2.3.0",
    "validate.js": "0.13.1"
  },
  "jest": {
    "setupTestFrameworkScriptFile": "<rootDir>__tests__/setup/setupEnzyme.js",
    "testPathIgnorePatterns": [
      "<rootDir>/__tests__/setup/"
    ],
    "moduleNameMapper": {
      "^RootDir(.*)$": "<rootDir>/src$1"
    }
  }
}
